{
    "name": "manzhouya/auth-attempts",
    "description": "Add captcha and login attempts for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "auth-attempts"],
    "homepage": "https://github.com/manzhouya/auth-attempts",
    "license": "MIT",
    "authors": [
        {
            "name": "manzhouya",
            "email": "manzhouya@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "mews/captcha": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Manzhouya\\AuthAttempts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manzhouya\\AuthAttempts\\AuthAttemptsServiceProvider"
            ]

        }
    }
}
